{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "PREBUILD_SCRIPT": "node scripts/prepare-build.js"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "PREBUILD_SCRIPT": "node scripts/prepare-build.js"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "PREBUILD_SCRIPT": "node scripts/prepare-build.js"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
